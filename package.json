{
  "name": "stimulant-gulpfile",
  "version": "0.0.1",
  "description": "The powerful gulpfile used by stimulant, now available as a standalone NPM package.",
  "author": "DSTIL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstil/stimulant-gulpfile.git"
  },
  "bugs": {
    "url": "https://github.com/dstil/stimulant-gulpfile/issues"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist && babel lib -d dist",
    "precommit": "npm test",
    "prepush": "npm test",
    "prepublish": "npm test && npm run build",
    "test": "mocha --compilers js:babel-register test/index.js"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.2.0",
    "browser-sync": "2.11.2",
    "browserify": "13.0.0",
    "del": "2.2.0",
    "get-wanted-dependencies": "1.1.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-filter": "4.0.0",
    "gulp-if": "2.0.0",
    "gulp-inject": "4.0.0",
    "gulp-minify-css": "1.2.4",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-rev": "7.0.0",
    "gulp-sass": "2.2.0",
    "gulp-uglify": "1.5.3",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.5",
    "rememberify": "1.0.1",
    "run-sequence": "1.1.5",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-register": "6.7.2",
    "husky": "0.11.4",
    "mocha": "2.4.5",
    "rimraf": "2.5.2",
    "should": "8.3.0"
  },
  "keywords": [
    "gulp",
    "gulpfile",
    "stimulant",
    "react",
    "sass",
    "babel",
    "build"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}

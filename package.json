{
  "name": "stimulant-gulpfile",
  "version": "1.2.8",
  "description": "The powerful gulpfile used by stimulant, now available as a standalone NPM package.",
  "author": "DSTIL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstil/stimulant-gulpfile.git"
  },
  "bugs": {
    "url": "https://github.com/dstil/stimulant-gulpfile/issues"
  },
  "main": "dist",
  "scripts": {
    "build": "node_modules/.bin/babel lib -d dist -q",
    "prebuild": "node_modules/.bin/rimraf dist",
    "precommit": "npm test",
    "prepush": "npm test",
    "prepublish": "npm test && npm run build",
    "pretest": "node_modules/.bin/eslint -c dstil lib",
    "test": "node_modules/.bin/mocha --compilers js:babel-register"
  },
  "dependencies": {
    "babel-plugin-transform-node-env-inline": "lukehorvat/babel-plugin-transform-node-env-inline#b6034b4",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "del": "2.2.2",
    "eslint-config-dstil-react": "1.0.1",
    "eslintify": "3.1.0",
    "express": "4.14.0",
    "get-wanted-dependencies": "1.2.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-clean-css": "2.0.13",
    "gulp-if": "2.0.1",
    "gulp-inject": "4.1.0",
    "gulp-livereload": "3.8.1",
    "gulp-rename": "1.2.2",
    "gulp-rev": "7.1.2",
    "gulp-sass": "2.3.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.10",
    "rememberify": "1.0.1",
    "run-sequence": "1.2.2",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-polyfill": "6.16.0",
    "babel-register": "6.18.0",
    "husky": "0.11.9",
    "list-directory-contents": "0.0.3",
    "mocha": "3.1.2",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "rimraf": "2.5.4",
    "should": "11.1.1"
  },
  "keywords": [
    "gulp",
    "gulpfile",
    "stimulant",
    "react",
    "sass",
    "babel",
    "build"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
